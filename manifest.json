{
  "name": "eMunch improved",
  "version": "0.3.0",
  "manifest_version": 3,
  "description": "emunch.no improved",
  "homepage_url": "https://github.com/samsonv/emunch-improved",
  "host_permissions": [
    "https://www.emunch.no/*",
    "https://emunch.no/*"
  ],
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["https://*.emunch.no/*"],
      "css": ["chat.css"],
      "js": ["script.js"]
    }
  ]
}