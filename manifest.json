{
  "name": "eMunch improved",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "emunch.no improved",
  "homepage_url": "https://github.com/samsonv",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "https://www.emunch.no/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.emunch.no/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}